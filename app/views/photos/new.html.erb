<%= form_for @photo do |n| %>
	店家名稱:<%= n.text_field :name, required: true %></br>
	縣/市:<%= n.text_field :city, required: true %></br>
	區域:<%= n.text_field :zone, required: true %></br>
	圖片:<%= n.file_field :image, required: true %></br>
	<%= n.submit '送出' %>
	<%= link_to('返回', root_url) %>
<% end %>
